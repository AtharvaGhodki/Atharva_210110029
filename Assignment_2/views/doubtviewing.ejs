<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>ICC-Doubt Viewing Section</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="css/style.css" />
  </head>
  <body>
    <div class="container mt-5">
      <h1 class="gap-4">Doubt Viewing Section</h1>
      <%doubtList.forEach(doubtview=>{%>

      <div
        class="card"
        style="width: 50rem; background-color: rgb(235, 211, 215)"
      >
        <div class="card-body" style="color: rgb(102, 5, 102)">
          <h5 class="card-title">
            <%=doubtview.firstname%> <%=doubtview.lastname%>
          </h5>

          <h6
            class="card-subtitle mb-2 text-muted"
            style="color: rgb(219, 134, 219)"
          >
            Doubt to be Resolve->
          </h6>
          <p class="card-text"><%=doubtview.doubt%></p>
        </div>
      </div>

      <%})%>
    </div>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
      crossorigin="anonymous"
    ></script>
  </body>
</html>
